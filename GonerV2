-- Criar a tool
local tool = Instance.new("Tool")
tool.Name = "Levantar Mão"
tool.RequiresHandle = true

-- Criar o handle invisível
local handle = Instance.new("Part")
handle.Name = "Handle"
handle.Size = Vector3.new(1, 1, 1)  -- Tamanho do handle
handle.Transparency = 1  -- Torna o handle invisível
handle.CanCollide = false  -- O handle não colide com outros objetos
handle.Anchored = false
handle.Parent = tool

-- Colocar a tool na backpack do jogador
tool.Parent = backpack
end
wait(0.1)
local player = game.Players.LocalPlayer
local backpack = player.Backpack

-- Cria a Tool
local tool = Instance.new("Tool")
tool.Name = "Som 1"
tool.RequiresHandle = false
tool.Parent = backpack

-- Função ao equipar a Tool
tool.Equipped:Connect(function()
    local id = 2291326370 -- Substitua pelo ID que você deseja usar
    local args = {
        [1] = "PickingScooterMusicText",
        [2] = id
    }
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1NoMoto1rVehicle1s"):FireServer(unpack(args))
end)

-- Função ao desequipar a Tool
tool.Unequipped:Connect(function()
    local args = {
        [1] = "PickingScooterMusicStop"
    }
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1NoMoto1rVehicle1s"):FireServer(unpack(args))
end)
wait(0.1)
