local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Função para parar todos os emotes ou animações em execução
local function stopAllEmotes()
    for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
        track:Stop()
    end
end

-- Chama a função para parar todos os emotes
stopAllEmotes()

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Função para definir a velocidade do personagem
local function setWalkSpeed(speed)
    humanoid.WalkSpeed = speed
end

-- Definindo a velocidade como 0
setWalkSpeed(16)
wait(0.1)
local player = game.Players.LocalPlayer

-- Função a ser executada após o renascimento do personagem
local function onCharacterAdded(character)
    -- Espera o personagem ter o Humanoid
    local humanoid = character:WaitForChild("Humanoid")

    -- Executa as funções após o renascimento

    -- Função 1
    local args1 = {
        [1] = "OCA"
    }
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Avata1rOrigina1l"):FireServer(unpack(args1))

    -- Função 2
    local args2 = {
        [1] = "RequestingEmmitersName",
        [2] = "0001Remove"
    }
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Clea1rTool1s"):FireServer(unpack(args2))
end

-- Conecta o evento CharacterAdded ao jogador
player.CharacterAdded:Connect(onCharacterAdded)
